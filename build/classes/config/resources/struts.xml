    	<action name="logout" 	class="com.cmsz.cloudplatform.web.action.LogoutAction"/>
    	<action name="getTemplateAccount" class="com.cmsz.cloudplatform.web.action.TemplateAccountAction" method="getTemplateAccount"/>
